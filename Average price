# Import NumPy
import numpy as np

# Define a 3x3 array of product prices (each row = one product's sales)
sales_data = np.array([
    [200, 250, 220],   # Product 1
    [300, 310, 305],   # Product 2
    [150, 180, 170]    # Product 3
])

# Calculate the average price of all products sold
average_price = np.mean(sales_data)

# Display the result
print(f"Average price of all products sold: â‚¹{average_price:.2f}")
